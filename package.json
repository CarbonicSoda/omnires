{
	"name": "omnires",
	"author": "CarbonicSoda",
	"version": "1.0.3",
	"description": "Universal JavaScript Object Renderer",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/CarbonicSoda/omnires"
	},
